{% extends "base.html.twig" %}

{% block title %}Card{% endblock %}

{% block body %}

<h1>Card</h1>

<div class="cards-container">
    {% for card in cards %}
        
        <img src="{{ asset('img/cards/' ~ card ~ '.png') }}" class="card">
        
    {% endfor %}
</div>



{{ dump(cards) }}

{% endblock %}